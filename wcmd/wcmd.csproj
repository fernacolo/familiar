<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <LangVersion>7.3</LangVersion>
    <RootNamespace>wcmd</RootNamespace>
    <VersionPrefix>0.8.9</VersionPrefix>
    <Title>Familiar</Title>
    <Authors>Fernando Colombo</Authors>
    <Description>The Familiar is a loyal friend to command line adventurers.</Description>
    <PackageProjectUrl>https://github.com/fernacolo/familiar</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/fernacolo/familiar/blob/master/LICENSE</PackageLicenseUrl>
    <PackageType>DotnetTool</PackageType>
    <!--PackAsTool>true</PackAsTool-->
    <RepositoryUrl>https://github.com/fernacolo/familiar.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.4.3" />
    <PackageReference Include="System.Diagnostics.EventLog" Version="4.5.0" />
  </ItemGroup>

</Project>